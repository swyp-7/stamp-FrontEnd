{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "prettier/prettier": "off",
    "no-unused-vars": "warn",
    "react/react-in-jsx-scope": "off",
    "prefer-const": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "no-constant-condition": "off"
  }
}
